<log4net>
  <appender name="UnityAppender" type="UnityAppender">
    <layout type="log4net.Layout.PatternLayout,log4net">
      <param name="ConversionPattern" value="%timestamp %level %class: %message%newline"/>
    </layout>
  </appender>
  
  <root>
    <level value="WARN"/>
    <appender-ref ref="UnityAppender"/>
  </root>
</log4net>
